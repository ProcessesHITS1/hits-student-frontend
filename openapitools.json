{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.6.0",
    "generators": {
      "chats": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:8010/swagger/v1/swagger.json",
        "output": "./src/api/clients/chats",
        "additionalProperties": {
          "supportsES6": true
        }
      },
      "auth": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:8080/v3/api-docs",
        "output": "./src/api/clients/auth",
        "additionalProperties": {
          "supportsES6": true
        }
      },
      "companies": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:8090/v3/api-docs",
        "output": "./src/api/clients/companies",
        "additionalProperties": {
          "supportsES6": true
        }
      },
      "interview": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:8081/swagger/v1/swagger.json",
        "output": "./src/api/clients/interview",
        "additionalProperties": {
          "supportsES6": true
        }
      }
    }
  }
}
